				[player, _arsenalWhitelist, false] call ace_arsenal_fnc_addVirtualItems;
				
				// Open ACE Arsenal
				_personalArsenal = [player, player, false] call ace_arsenal_fnc_openBox;

	};


	//------------------------------------------------------------------
	//------------------------------------------------------------------
	//
	//						Zug (filtered)
	//
	//------------------------------------------------------------------
	//------------------------------------------------------------------	
	
	default {

				// Get Player Role
				_loadout = player getVariable "loadout";

				//Needed for waitUntil after Arsenal is closed, combined with Event Handler on Arsenal
				player setVariable ["arsenalClosed", false];

				// Getting All Arsenal Items together

				_personalArsenalItems = zug_uniform + zug_helm + zug_rucksack + zug_waffen_items + zug_equipment + facewear;


				//------------------------------------------------------------------
				//------------------------------------------------------------------
				//
				//						Führung
				//
				//------------------------------------------------------------------
				//------------------------------------------------------------------	
					
				if (_loadout == "Zug_Truppfuhrer" || _loadout == "Zug_Gruppenfuhrer") then {
					
					_personalArsenalItems = _personalArsenalItems + zug_fuehrungskraefte;
				};
							
				
				// Fill ACE Arsenal
				[player, _personalArsenalItems, false] call ace_arsenal_fnc_addVirtualItems;
				
				// Open ACE Arsenal
				_personalArsenal = [player, player, false] call ace_arsenal_fnc_openBox;

				// Give Warning about Ammunition
				titleText ["<t color='#ffffff' size='1'>In diesem Arsenal könnt ihr nur euer Aussehen anpassen.<br/>Für Waffenwechsel bitte zur Waffenkammer gehen.</t>", "PLAIN", 1, true, true];

	};
};